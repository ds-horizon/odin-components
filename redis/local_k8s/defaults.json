{
  "deployment": {
    "mode": "standalone",
    "config": {}
  },
  "master": {
    "resources": {
      "requests": {
        "cpu": "500m",
        "memory": "1Gi"
      },
      "limits": {
        "cpu": "1000m",
        "memory": "2Gi"
      }
    }
  },
  "persistence": {
    "enabled": true,
    "storageClass": "",
    "size": "10Gi",
    "rdb": {
      "enabled": true,
      "saveInterval": "900 1 300 10 60 10000"
    },
    "aof": {
      "enabled": false,
      "fsyncPolicy": "everysec"
    }
  },
  "service": {
    "type": "ClusterIP",
    "annotations": {}
  },
  "updateStrategy": {
    "type": "RollingUpdate",
    "rollingUpdate": {
      "maxUnavailable": 1
    }
  },
  "nodeSelector": {},
  "additionalConfig": {}
}
